{
  "eslint.validate": ["yaml", "json", "typescript", "javascript"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always"
  },
  "cSpell.words": [
    "amoy",
    "ANTT",
    "BIOW",
    "carrot",
    "CETESB",
    "claude",
    "clickup",
    "EIP-712",
    "ethereum",
    "ipfs",
    "IPNS",
    "ipns",
    "keccak256",
    "MassID",
    "offchain",
    "UNFCCC",
    "usdc",
    "NFTIPFSSchema",
    "mathieudutour",
    "softprops",
    "uuidv4",
    "massIDIPFSSchema",
    "massIDIpfsSchema",
    "massIDDataSchema",
    "baseIpfsSchema",
    "IpfsUri",
    "bafybeigdyrzt",
    "efuylqabf",
    "oclgtqy",
    "fbzdi"
  ],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
